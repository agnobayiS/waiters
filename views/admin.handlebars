<body>


    <form action="/admin" method="get">

        <div class="containe">

            {{#if messages.erro}}
            <div style="height:30px ;" class="massages">
                <h3 style="color:red; font-size:15px " class="massages">{{messages.erro}}</h3>
            </div>{{/if}}


        </div>
        <div class="containe">
            <div>
                <div class="table ">

                    <table>
                        <tr>
                            <th>Monday</th>
                            <th>Tuesday</th>
                            <th>Wednesday</th>
                            <th>Thursday</th>
                            <th>Friday</th>
                            <th>Saterday</th>
                            <th>Sunday</th>
                        </tr>
                        {{#each data}}
                        <tr>
                            <td>{{this.names}}</td>

                        </tr>
                        {{/each}}
                    </table>


                </div>

            </div>
        </div>
    </form>
    {{#each data}}

    <form action="/resert/{{this.names_id}}" method="get">
        {{/each}}

        <div style="padding-top: 40px ;">
            <button class="btn" type="resert">RESERT DATA</button>
        </div>
    </form>

</body>